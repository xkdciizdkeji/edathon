# Three-valued logic circuit (BENCH format)

INPUT(i0)
INPUT(i1)
INPUT(i2)
INPUT(i3)
INPUT(i4)
INPUT(i5)
INPUT(i6)
INPUT(i7)
INPUT(i8)
INPUT(i9)

OUTPUT(o0)
OUTPUT(o1)
OUTPUT(o2)
OUTPUT(o3)

o0 = MOD3_ADD(MIN(i8, i1), i0)
o1 = MOD3_MUL(MOD3_ADD(i9, i1), i3)
o2 = MIN(MOD3_MUL(i4, i2), MAX(i7, i8))
o3 = MOD3_ADD(MOD3_MUL(MOD3_ADD(i9, i1), i3), MOD3_ADD(MIN(i8, i1), i0))